<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.androidbootcamp.coffeefinder" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >

        <activity
            android:name=".Start"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
        </activity>

        <activity
            android:name=".Starbucks"
            android:label="@string/title_activity_starbucks" >
        </activity>
        <activity
            android:name=".BreadFurst"
            android:label="@string/title_activity_bread_furst" >
        </activity>
        <activity
            android:name=".Politics"
            android:label="@string/title_activity_politics" >
        </activity>
        <activity
            android:name=".CoffeeNature"
            android:label="@string/title_activity_coffee_nature" >
        </activity>

    </application>

</manifest>
